package com.shristi.training;

public class IfElseDemo {

	public static void main(String[] args) {
		// Find the greater of 3 numbers use-&&
		int num1 = 100;
		int num2 = 90;
		int num3 = 120;

		String name = "Sri";
		if (name.equals("Sri")) {
			System.out.println("Welcome " + name);
		} else {
			System.out.println("wrong username");
		}
	}
}
